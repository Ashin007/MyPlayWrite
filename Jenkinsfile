pipeline {
    agent any

    tools {
        maven 'Local_Maven'
        jdk 'Local_Java'
    }

    stages {
        stage('Checkout') {
            steps {
                checkout scm
            }
        }

        stage('Build') {
            steps {
                bat 'mvn clean package'
            }
        }

        stage('Test') {
            steps {
                bat 'mvn test'
            }
        }


    }


}
